#NEXUS
Begin paup;
	set autoclose=yes warntree=no warnreset=no;
	execute seqs_844G_no_gaps.nexus; #(Refurbish: Give the appropriate alignment in nexus format)
	dset distance=GTR basefreq=empirical rates=gamma shape=0.70 pinv=0.66; #(Refurbish: Give appropriate shape= and pinv to your model. YOu will find this parameters at the end of Jmodeltest/nohup.out)
	nj brlens;
	outgroup 16G_epiroticus_SAMN01087920; #(Refurbish: Give the fasta header of your outgroup)
	savetrees file=844G.tre brlens format=altnexus; #(Refurbish: Give the out put name of the file that will contain the tree)
	q;
end;
